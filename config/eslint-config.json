{
	"ecmaFeatures": {
		"jsx": true
	},
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"rules": {
		"comma-dangle": 2,
		"no-cond-assign": 2,
		"no-constant-condition": 2,
		"no-control-regex": 2,
		"no-debugger": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-empty": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-sparse-arrays": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-typeof": 2,
		"no-unexpected-multiline": 2,
		"prefer-spread": 1,
		"brace-style": 1,
		"camelcase": 1,
		"comma-style": 1,
		"consistent-this": [
			1, "self"
		],
	"no-inline-comments": 1,
	"no-nested-ternary": 1,
		"no-trailing-spaces": 1,
		"no-underscore-dangle": 1,
		"space-before-function-paren": 1,
		"space-infix-ops": 1,
		"space-return-throw-case": 1,
		"arrow-spacing": 2,
		"constructor-super": 2,
		"no-const-assign": 2,
		"no-this-before-super": 2,
		"new-parens": 2,
		"no-array-constructor": 2,
		"no-lonely-if": 2,
		"no-unneeded-ternary": 2,
		"one-var": [
			2, "never"
		],
		"callback-return": [
			1, ["callback", "next"]
		],
		"no-shadow": 1,
		"no-unused-vars": 1,
		"no-new-require": 2,
		"no-path-concat": 2,
		"no-delete-var": 2,
		"handle-callback-err": 2,
		"no-shadow-restricted-names": 2,
		"dot-notation": 1,
		"guard-for-in": 1,
		"no-caller": 1,
		"no-else-return": 1,
		"no-extend-native": 1,
		"no-floating-decimal": 1,
		"no-lone-blocks": 1,
		"no-param-reassign": 1,
		"no-proto": 1,
		"no-sequences": 1,
		"no-void": 1,
		"no-with": 1,
		"eqeqeq": 2,
		"no-alert": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-extra-bind": 2,
		"no-fallthrough": 2,
		"no-implicit-coercion": 2,
		"no-implied-eval": 2,
		"no-loop-func": 2,
		"no-native-reassign": 2,
		"no-new-func": 2,
		"no-octal": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-useless-call": 2,
		"no-unused-expressions": 2,
		"radix": 2,
		"yoda": 2
	}
}
